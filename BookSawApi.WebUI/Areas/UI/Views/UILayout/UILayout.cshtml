
@{
    Layout = null;
}


<!DOCTYPE html>
<html lang="en">

<head>

	@await Component.InvokeAsync("_HeadDefaultComponentPartial")
</head>

<body data-bs-spy="scroll" data-bs-target="#header" tabindex="0">

	
	@await Component.InvokeAsync("_NavbarComponentPartial")

	@await Component.InvokeAsync("_FeaturComponentPartial")	

	<section id="client-holder" data-aos="fade-up">
		<div class="container">
			<div class="row">
				<div class="inner-content">
					<div class="logo-wrap">
						<div class="grid">
							<a href="#"><img src="/booksaw-1.0.0/images/client-image1.png" alt="client"></a>
							<a href="#"><img src="/booksaw-1.0.0/images/client-image2.png" alt="client"></a>
							<a href="#"><img src="/booksaw-1.0.0/images/client-image3.png" alt="client"></a>
							<a href="#"><img src="/booksaw-1.0.0/images/client-image4.png" alt="client"></a>
							<a href="#"><img src="/booksaw-1.0.0/images/client-image5.png" alt="client"></a>
						</div>
					</div><!--image-holder-->
				</div>
			</div>
		</div>
	</section>

	
	@await Component.InvokeAsync("_LastFourBookComponentPartial")


	@await Component.InvokeAsync("_RandomBookComponentPartial")

	@await Component.InvokeAsync("_BookListComponentPartial")


	@await Component.InvokeAsync("_RandomQuoteComponentPartial")



	@await Component.InvokeAsync("_AuthorListComponentPartial")
	
	

	@await Component.InvokeAsync("_FooterComponentPartial")

	<script src="/booksaw-1.0.0/js/jquery-1.11.0.min.js"></script>
	<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js"
			integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm"
			crossorigin="anonymous"></script>
	<script src="/booksaw-1.0.0/js/plugins.js"></script>
	<script src="/booksaw-1.0.0/js/script.js"></script>

</body>

</html>